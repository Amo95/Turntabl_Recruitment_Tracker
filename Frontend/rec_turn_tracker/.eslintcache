ginImg from \"../../signup.svg\";\nimport { register } from '../../service/actions/actions';\nimport { connect } from 'react-redux'\nimport { message } from 'antd';\n\nclass Register extends React.Component {\n  constructor(props) {\n    super(props);\n  }\n\n  state = {\n    email: \"\",\n    password: \"\",\n    confirmPassword: \"\",\n  }\n\n  onchangeHandler = (event) => {\n    this.setState({ [event.target.name]: event.target.value });\n  }\n\n  submitHandler = (event) => {\n    const { email, password, confirmPassword } = this.state\n    this.props.dispatch(register(email, password, confirmPassword))\n    \n    this.props.status ? message.success(this.props.message) : message.error(this.props.message)\n  }\n  \n  render() {\n    const { email, password, confirmPassword } = this.state\n\n    return (\n      <div className=\"base-container\" ref={this.props.containerRef}>\n        <div className=\"header\">Register</div>\n        <div className=\"content\">\n          <div className=\"image\">\n            <img src={loginImg} />\n          </div>\n          <div className=\"form\">\n            <div className=\"form-group\">\n              <label htmlFor=\"email\">Email</label>\n              <input type=\"email\" name=\"email\" placeholder=\"Enter Email\" value={email} onChange={this.onchangeHandler} required />\n            </div>\n            <div className=\"form-group\">\n              <label htmlFor=\"email\">Password</label>\n              <input type=\"password\" name=\"password\" placeholder=\"Enter Password\" value={password} onChange={this.onchangeHandler} required />\n            </div>\n            <div className=\"form-group\">\n              <label htmlFor=\"password\">Password Confirmation</label>\n              <input type=\"password\" name=\"confirmPassword\" placeholder=\"Confirm Password\" value={confirmPassword} onChange={this.onchangeHandler} required />\n            </div>\n          </div>\n        </div>\n        <div className=\"footer\">\n          <button type=\"submit\" className=\"button\" onClick={this.submitHandler}>\n            Register\n          </button>\n        </div>\n\n      </div>\n    );\n  }\n}\n\nconst mapStateToProps = state => ({\n  status: state.Register.status,\n  message: state.Register.message\n});\n\nexport default connect(mapStateToProps)(Register)\n","/home/mawulorm/Projrcts/Turntabl-exe/Turntabl_Recruitment_Tracker/Frontend/rec_turn_tracker/src/components/applicant/login.jsx",["90","91"],"import React from \"react\";\nimport loginImg from \"../../login.svg\";\nimport { connect } from 'react-redux'\n// import { loginUser } from '../../service/service'\n\nimport { loginInit, loginUser } from '../../service/actions/actions'\nimport { Redirect } from \"react-router-dom\";\nclass Login extends React.Component {\n  constructor(props) {\n    super(props);\n\n    this.state = {\n      email: \"\",\n      password: \"\"\n    }\n  }\n\n  onChangeHandler = (event) => {\n    this.setState({ [event.target.name]: event.target.value });\n  }\n\n  onSubmitHandler = (e) => {\n    e.preventDefault()\n    const { email, password } = this.state\n    this.props.dispatch(loginUser(email, password))\n  }\n  render() {\n    const { email, password } = this.state\n    if (this.props.login_stat) {\n      localStorage.setItem(\"isLogedIn\", this.props.login_stat)\n      return <Redirect to=\"/applyv2\" />\n    }\n    else if (localStorage.getItem(\"isLogedIn\") !== null) {\n      return <Redirect to=\"/applyv2\" />\n    }\n    else {\n      return (\n        <form onSubmit={this.onSubmitHandler}>\n          <div className=\"base-container\" ref={this.props.containerRef}>\n            <div className=\"header\">Login</div>\n            <div className=\"content\">\n              <div className=\"image\">\n                <img src={loginImg} />\n              </div>\n              <div className=\"form\">\n                <div className=\"form-group\">\n                  <label htmlFor=\"email\">Email</label>\n                  <input type=\"email\" name=\"email\" placeholder=\"Enter Email\" onChange={this.onChangeHandler} value={email} required />\n                </div>\n                <div className=\"form-group\">\n                  <label htmlFor=\"email\">Password</label>\n                  <input type=\"password\" name=\"password\" placeholder=\"Enter Password\" onChange={this.onChangeHandler} value={password} required />\n                </div>\n              </div>\n            </div>\n            <div className=\"footer\">\n              <button type=\"submit\" className=\"button\">\n                Login\n            </button>\n            </div>\n            <a href=\"http://\" target=\"_blank\" rel=\"noopener noreferrer\"> <b>Forgot Password?</b> </a>\n          </div>\n        </form>\n      );\n    }\n  }\n}\nconst mapStateToProps = state => ({\n  uId: state.isLogin.id,\n  uEmail: state.isLogin.email,\n  login_stat: state.isLogin.login_status\n});\n\nexport default connect(mapStateToProps)(Login)\n","/home/mawulorm/Projrcts/Turntabl-exe/Turntabl_Recruitment_Tracker/Frontend/rec_turn_tracker/src/components/portal/footer.jsx",["92"],{"ruleId":"93","severity":1,"message":"94","line":6,"column":23,"nodeType":"95","messageId":"96","endLine":6,"endColumn":38},{"ruleId":"93","severity":1,"message":"97","line":6,"column":40,"nodeType":"95","messageId":"96","endLine":6,"endColumn":47},{"ruleId":"93","severity":1,"message":"98","line":9,"column":8,"nodeType":"95","messageId":"96","endLine":9,"endColumn":13},{"ruleId":"99","replacedBy":"100"},{"ruleId":"101","replacedBy":"102"},{"ruleId":"93","severity":1,"message":"103","line":2,"column":42,"nodeType":"95","messageId":"96","endLine":2,"endColumn":50},{"ruleId":"93","severity":1,"message":"104","line":4,"column":10,"nodeType":"95","messageId":"96","endLine":4,"endColumn":14},{"ruleId":"105","severity":1,"message":"106","line":48,"column":13,"nodeType":"107","endLine":48,"endColumn":31},{"ruleId":"105","se